{
  "name": "loop.js",
  "version": "0.2.2",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "browser": "mkdir -p browser && browserify index.js --standalone loop -p [minifyify --no-map] > browser/loop.js",
    "build": "babel index.js -d dist/",
    "size": "bundle-size ./dist/index.js",
    "lint": "standard 'src/*.js'",
    "prebuild": "npm run lint",
    "test": "ava test -v"
  },
  "browserify": {
    "debug": true,
    "cache": {},
    "packageCache": {},
    "extension": [
      "js"
    ],
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/estrattonbailey/loop.js.git"
  },
  "keywords": [
    "events",
    "emitter",
    "pub/sub"
  ],
  "author": "estrattonbailey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/estrattonbailey/loop.js/issues"
  },
  "homepage": "https://github.com/estrattonbailey/loop.js#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.22.1",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "babili": "0.0.10",
    "browserify": "^13.0.1",
    "bundle-size": "^0.7.0",
    "jsdom": "^9.4.1",
    "jsdom-global": "^2.0.0",
    "minifyify": "^7.3.3",
    "query-string": "^4.2.3",
    "standard": "^8.6.0",
    "watchify": "^3.7.0"
  }
}
